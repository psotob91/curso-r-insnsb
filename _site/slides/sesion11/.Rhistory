library(MASS)
library(tidyverse)
library(rio)
library(ggeffects)
library(car)
library(statmod)
library(broom)
library(simglm)
set.seed(2018)
n <- 200
x1 <- rnorm(n, 0, 1)
x2 <- runif(n, -2, 2)
beta1 <- -1
beta2 <- 1
nu_i <- 4 + beta1 * x1 + beta2 * x2 ^ 2
mu_i <- exp(nu_i)
y_i <- rpois(n, nu_i)
datos <- data.frame(
y_i = y_i,
x1 = x1,
x2 = x2
)
mod <- glm(y_i ~ x1 + x2,
family = poisson(link = "log"),
data = datos)
summary(mod)
xb_x1 <- ggpredict(mod, "x1 [all]")
plot(xb_x1, residuals = TRUE, residuals.line = TRUE)
xb_x2 <- ggpredict(mod, "x2 [all]")
plot(xb_x2, residuals = TRUE, residuals.line = TRUE)
termplot(mod, partial.resid = TRUE)
ceresPlots(mod)
datos[c("part_res_x1", "part_res_x2")] <- residuals(mod, "partial")
export(datos, "poi_x_2_indep.dta")
